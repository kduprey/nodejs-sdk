{
  "name": "@byteflow-inc/sdk",
  "version": "1.3.5",
  "description": "This is an SDK for Byteflow. An SMS messaging provider that let's you start sending messages without the wait.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "version": "npm run format && git add -A src",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json"
  },
  "keywords": [
    "sms",
    "messaging",
    "sdk",
    "byteflow"
  ],
  "author": "Bryght, LLC",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.4.0",
    "fs": "^0.0.1-security",
    "nanoid": "3.3.6",
    "qrate": "^1.2.1",
    "throttled-queue": "^2.1.4"
  },
  "devDependencies": {
    "prettier": "^3.0.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.1.6"
  },
  "files": [
    "lib/**/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/ByteflowInc/sdk",
  "repository": {
    "type": "git",
    "url": "https://github.com/ByteflowInc/sdk.git"
  }
}
